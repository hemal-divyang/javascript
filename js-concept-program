// string is immutable (not able to be changed) - stores in memory location
//object and array is mutable(able to be changed) - store a reference to the location in memory

let str = 'JS';
name[0] = 'R';
name[1] = 'X'
console.log(str)

let arr = ['a','b']
arr[0]='c'
arr[1]='d'
console.log(arr)

let obj = {name:'hemal',sal:2000}
obj.name = 'ved'
console.log(obj.name)
