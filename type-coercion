console.log(10 + '20');//1020
console.log('20' + 10);//2010
console.log(true + '10');//true10
console.log(false + 2);//2
console.log(10 - '5');//5
console.log(10 * '5');//50
console.log(10 / '5');//2
console.log(10 % '5');//0
console.log(true == 1);//true
console.log(true === 1);//false
console.log([] === 0);//false
console.log([] == 0);//true
console.log([] == 1);//false
console.log([] === 1);//false
